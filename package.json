{
  "private": true,
  "name": "@acme/monorepo",
  "scripts": {
    "dev": "turbo dev",
    "dev:example": "turbo dev --filter=\"{./apps/example}...\"",
    "dev:storybook": "turbo dev --filter=\"{./apps/ui-library}...\"",
    "lint": "turbo lint",
    "test": "turbo test",
    "format": "prettier --write .",
    "build": "turbo build",
    "build:example": "turbo build --filter=\"...{./apps/example}\"",
    "lint:repo": "sherif"
  },
  "devDependencies": {
    "sherif": "^1.5.0",
    "turbo": "^1.13.4",
    "typescript": "^5.8.3"
  },
  "pnpm": {
    "overrides": {
      "react-docgen-typescript": "2.2.2"
    }
  },
  "prettier": {},
  "packageManager": "pnpm@9.12.1"
}
